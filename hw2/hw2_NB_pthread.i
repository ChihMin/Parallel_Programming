      pthread_mutex_unlock(&mutex[i]);

      XSetForeground(display, gc, ((&((_XPrivDisplay)display)->screens[screen])->white_pixel)); XDrawPoint (display, window, gc, xPix, yPix);;
    }
    usleep(1000000/10);
    XFlush(display);
  }
  for (int i = 0; i < threads; ++i)
    pthread_join(pthreads[i], __null);


