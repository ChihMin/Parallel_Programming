void *thread_func(void *ID) {
  long threadID = (long)ID;

  long numPerThreads = N / threads;
  long beginIndex = numPerThreads * threadID;
  if (threadID == threads - 1) {
    numPerThreads += N % threads;
  }

  int STEPS = T;
  while (STEPS--) {
    printf("[%d] STEP = %d\n", threadID, STEPS);
    for (long i = beginIndex, j = 0; j < numPerThreads; ++i, ++j) {
      double ax_sum = 0;
      double ay_sum = 0;

      pthread_mutex_lock(&mutex[i]);
       Node n1 = node[i];
      pthread_mutex_unlock(&mutex[i]);
      printf("BEFORE ---> ID[%d, %d] = (%lf, %lf, %lf, %lf)\n", threadID, i, node[i].x, node[i].y, node[i].vx, node[i].vy);
      for (long j = 0; j < N; ++j) {
        if (i == j) continue;
        double x1, y1;
        double x2, y2;
        double ax ,ay;
        double dx, dy;


        pthread_mutex_lock(&mutex[j]);
          Node n2 = node[j];
        pthread_mutex_unlock(&mutex[j]);

        x1 = n1.x;
        y1 = n1.y;

        x2 = n2.x;
        y2 = n2.y;

        dx = x2 > x1 ? x2 - x1 : x1 - x2;
        dy = y2 > y1 ? y2 - y1 : y1 - y2;

        double dis = sqrt((pow(dx, 2)+pow(dy, 2)));
        if (dis < 0.000001) continue;

        ax = (((6.67384*pow(10, -11))*mass / (pow(dx, 2)+pow(dy, 2))) * (dx / sqrt((pow(dx, 2)+pow(dy, 2)))));
        ay = (((6.67384*pow(10, -11))*mass / (pow(dx, 2)+pow(dy, 2))) * (dy / sqrt((pow(dx, 2)+pow(dy, 2)))));

        if (x2 < x1) ax = -ax;
        if (y2 < y1) ay = -ay;

        printf("(%d <-> %d)(%.30lf,%.30lf)\n", i, j, ax, ay);

        ax_sum += ax;
        ax_sum += ay;
      }
      double x_next = n1.x + (n1.vx + ax_sum*times) * times;
      double y_next = n1.y + (n1.vy + ay_sum*times) * times;
      double Vx = (n1.vx + ax_sum*times);
      double Vy = (n1.vy + ay_sum*times);
      printf("AFTER  ---> ID[%d, %d] = (%lf, %lf, %lf, %lf, %lf, %lf)\n\n", threadID, i, x_next, y_next, Vx, Vy, ax_sum, ay_sum);
      pthread_mutex_lock(&mutex[i]);
        node[i] = Node(x_next, y_next, Vx, Vy);
      pthread_mutex_unlock(&mutex[i]);
    }
  }
  fprintf(stderr, "ID[%d] exit\n", threadID);
  pthread_exit(__null);
}


